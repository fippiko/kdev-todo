<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.mainPanel {
			height: 100%;
			width: 100%;
		}
		
		.cellTable{
			border-bottom: 1px solid #ccc;
			text-align: left;
			margin-bottom: 4px;
		}	
	</ui:style>
	<g:HTMLPanel styleName='{style.mainPanel}'>
		Manage Projects
		
		<br/>
		<br/>
		<br/>
		
		<g:Button ui:field="addProjectButton">Add Project</g:Button>
		<g:Button ui:field="viewProjectButton" enabled="false">View Project</g:Button>
		<g:Button ui:field="editProjectButton" enabled="false">Edit Project</g:Button>
		<g:Button ui:field="deleteProjectButton" enabled="false">Delete Project</g:Button>
		<br/>
		<br/>
		
		
		<g:ListBox ui:field="projectList" visibleItemCount="10"/>
		
		<g:FlowPanel ui:field="subViewPanel">
	
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>